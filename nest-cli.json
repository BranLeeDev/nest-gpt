{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "assets": ["common/modules/serve-static/**/*.{html,css}"],
    "watchAssets": true,
    "deleteOutDir": true,
    "builder": "swc",
    "typeCheck": true
  }
}
